#ignores some terraform state files and some secrets from commiting
.terraform/
*.tfstate 
*.tfstate.*
.terraform.tfstate.lock.info

#ignores logs from terraform crashes
crash.log
crash.*.log

#ignore some local overrides
override.tf 
override.tf.json 
*_override.tf
*_override.tf.json

#ignore var values
*.tfvars
*.tfvars.json
backend.tfvars

#ignore config details
.terraformrc
terraform.rc

#ignore zips that may be created 
infra/lambda/*/*.zip
infra/build/

# generated output artifacts
infra/out.json

#ignore tf plans
*.tfplan
*.plan

# local runtime/version files
.python-version

# optional screenshots/assets not for source control
frontWeb/*.png
frontWeb/*.jpg
screenshots/

#python and env files
__pycache__/
*.pyc
.venv/
.env

#just the usual IDE ignores
.DS_Store
.vscode/
.idea/
